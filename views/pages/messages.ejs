<div class="container">
    <div class="row">
        <div class="col-md-12">
            <table class="table table-hover table-bordered">
                <thead>
                    <th>#</th>
                    <th>Name</th>
                    <th>email</th>
                    <th>phone</th>
                    <th>message</th>
                </thead>
                <tbody>
        <% contacts.forEach((contact, index) => { %>
            <tr>
                <td><%= index + 1 %></td>
                <td><%= contact.name %></td>
                <td><%= contact.email %></td>
                <td><%= contact.phone %></td>
                <td><%= contact.message %></td>
            </tr>
        <% }) %> 
                 </tbody>
            </table>
        </div>
    </div>
</div>
<%- include('../layouts/footer.ejs') %> 